{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web"
    }
  ]
}
